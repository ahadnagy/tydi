impl compositions.Top_level {
   /*Instantiation example*/
   inst1 : primitives.Magic[param := 5];
   inst2 : primitives.Magic[param := 6];
   inst3 : primitives.Magic[param := 7];
   /*Bulk connection chain*/

   inst1.in <= this.in;
   inst2.in <= inst1.out;
   inst3.in <= inst2.out;
   this.out <= inst3.out;
}